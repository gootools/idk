{
  "manifest_version": 2,
  "name": "Goo",
  "version": "0.0.1",
  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking", "storage"],
  "browser_action": {
    "default_popup": "app/web-build/index.html"
  },
  "background": {
    "scripts": ["extension/background.js"],
    "persistent": true
  }
}
